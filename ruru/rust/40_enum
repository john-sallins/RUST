// For example, say we want to get values from a row in a spreadsheet in which some of the columns in the row contain integers, some 
// floating-point numbers, and some strings. 
// We can define an enum whose variants will hold the different value types.

    enum SpreadsheetCell {
        Int(i32),
        Float(f64),
        Text(String),
    }

    let row = vec![
        SpreadsheetCell::Int(3),
        SpreadsheetCell::Text(String::from("blue")),
        SpreadsheetCell::Float(10.12),
    ];
